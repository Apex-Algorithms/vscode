{
	"name": "joshbot",
	"displayName": "JoshBot",
	"publisher": "vscode",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.101.0"
	},
	"main": "./out/extension.js",
	"enabledApiProposals": [
		"remoteCodingAgents"
	],
	"contributes": {
		"commands": [
			{
				"command": "joshbot.remoteAgent",
				"title": "JoshBot: Remote Coding Agent"
			}
		],
		"remoteCodingAgents": [
			{
				"command": "joshbot.remoteAgent",
				"displayName": "JoshBot",
				"description": "A playful remote coding agent powered by JoshBot."
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.101.0",
		"typescript": "^5.8.3"
	}
}
